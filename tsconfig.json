{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@/*":["./src/*"],
      "@/framework/components/*":["./src/framework/components/*"],
      "@/framework/utils/*":["./src/framework/utils/*"],
      "@/assets/*":["./src/assets/*"],
      "@/pages/*":["./src/pages/*"],
      "@/models/*":["./src/models/*"],
      "@/service/*":["./src/service/*"],
      "@/types/*":["./src/types/*"],
    },
  },
  "include": ["./src", "./types"],
  "compileOnSave": false
}
